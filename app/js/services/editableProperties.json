[
    {
        "label": "Basic Display",
        "type": "menu",
        "description": "basic settings let you set bla bla bla\n lorem ipsum test goes here",
        "model": "basicDisplay",
        "icon": "TabBasicSettings",
        "children": [
            {
                "label": "Player's Name",
                "type": "text",
                "model": "name",
                "default": "New Player",
                "endline": "true"
            },
            {
                "label": "Player Tags",
                "type": "tags",
                "model": "tags",
                "source" :"getTags"
            },
            {
                "label": "Preview entry",
                "type": "select2data",
                "source": "listEntries",
                "query": "queryEntries",
                "endline": "true",
                "width": "100%",
                "model": "previewentry",
                "data-placeholder": "Pick a entry"
            },
            {
                "options": [
                    {
                        "label": "4/3",
                        "value": "narrow"
                    },
                    {
                        "label": "16/9",
                        "value": "wide"
                    }
                ],
                "showSearch": false,
                "initvalue": "wide",
                "type": "dropdown",
                "label": "Aspect Ratio",
                "endline": "true",
                "model": "aspectRatio"
            },
            {
                "type": "featuremenu",
                "model": "transport",
                "label": "Transport",
                "children": [
                    {
                        "options": [
                            {
                                "label": "2mbs",
                                "value": 2
                            },
                            {
                                "label": "1mbs",
                                "value": 1
                            }
                        ],
                        "initvalue": 1,
                        "type": "dropdown",
                        "label": "Prefered Bitrate",
                        "model": "preferedFlavorBR"
                    },
                    {
                        "options": [
                            {
                                "label": "option",
                                "value": "0.75"
                            }
                        ],
                        "initvalue": "0.75",
                        "type": "dropdown",
                        "label": "Delivery type",
                        "model": "deliveryType"
                    }
                ]
            },
            {
                "type": "featuremenu",
                "model": "deviceSpecific",
                "label": "Device Specific Flags",
                "children": [
                    {
                        "type": "checkbox",
                        "label": "Html Controls on iPad",
                        "model": "ipadHTMLcontrols"
                    },
                    {
                        "type": "checkbox",
                        "label": "Native Controls Fullscreen",
                        "model": "nativeFullscreenControls"
                    }
                ]
            },
            {
                "label": "Last Update",
                "type": "readonly",
                "model": "lastUpdated"
            },
            {
                "label": "Version:",
                "btn-label": "Update Player",
                "type": "infoAction",
                "btn-class": "btn-xs btn-primary",
                "action": "update",
                "model": "version"
            }
        ]
    },
    {
        "label": "Look and Feel",
        "icon": "TabLookandFeel",
        "description": "look and feel let you set bla bla bla\n lorem ipsum test goes here",
        "type": "menu",
        "model": "lookAndFeel",
        "children": [
            {
                "type": "menu",
                "model": "title",
                "label": "Title Label",
                "children": [
                    {
                        "options": [
                            {
                                "label": "Right",
                                "value": "right"
                            },
                            {
                                "label": "Center",
                                "value": "center"
                            },
                            {
                                "label": "Left",
                                "value": "left"
                            }
                        ],
                        "initvalue": "right",
                        "type": "radio",
                        "model": "align"
                    },
                    {
                        "options": [
                            {
                                "label": "option",
                                "value": "0.75"
                            }
                        ],
                        "initvalue": "0.75",
                        "type": "dropdown",
                        "label": "Delivery type",
                        "model": "deliveryType"
                    },
                    {
                        "label": "Template",
                        "model": "titleTemplate",
                        "default": "<span data-bind=”{mediaProxy.entry.name}”></span>",
                        "type": "modaltextbox"
                    },
                    {
                        "options": [
                            {
                                "label": "videoHolder",
                                "value": "videoHolder"
                            },
                            {
                                "label": "ContentHolder",
                                "value": "contentHolder"
                            }
                        ],
                        "initvalue": "videoHolder",
                        "type": "dropdown",
                        "label": "Container",
                        "model": "titleContainer"
                    },
                    {
                        "default": ".titleLabel",
                        "model": "titleCSSclass",
                        "label": "CSS class",
                        "type": "readonly"
                    }
                ]
            },
            {
                "type": "menu",
                "model": "controlBar",
                "label": "Control Bar",
                "children": [
                    {
                        "label": "Size",
                        "showTextEntry": true,
                        "type": "slider",
                        "default": "100%,30px",
                        "model": "controlBarSize"
                    },
                    {
                        "type": "featuremenu",
                        "model": "controlBarVolume",
                        "label": "Volume Control",
                        "children": [
                            {
                                "options": [
                                    {
                                        "label": "Horizontal",
                                        "value": "0"
                                    },
                                    {
                                        "label": "Vertical",
                                        "value": "1"
                                    }
                                ],
                                "type": "radio",
                                "initvalue": "0",
                                "model": "controlBarVolumeDirection"
                            },
                            {
                                "label": "Initial Volume",
                                "type": "number",
                                "default": 80,
                                "model": "initialvolume",
                                "from": 0,
                                "to": 100,
                                "stepsize": 5
                            }
                        ]
                    },
                    {
                        "type": "featuremenu",
                        "model": "logo",
                        "label": "Logo",
                        "order": 0,
                        "children": [
                            {
                                "options": [
                                    {
                                        "label": "Right",
                                        "value": "right"
                                    },
                                    {
                                        "label": "Center",
                                        "value": "center"
                                    },
                                    {
                                        "label": "Left",
                                        "value": "left"
                                    }
                                ],
                                "initvalue": "right",
                                "type": "radio",
                                "model": "align"
                            },
                            {
                                "model": "priority",
                                "label": "Display Priority",
                                "options": [
                                    {
                                        "label": "Low",
                                        "value": "0"
                                    },
                                    {
                                        "label": "Medium",
                                        "value": "1"
                                    },
                                    {
                                        "label": "High",
                                        "value": "2"
                                    }
                                ],
                                "initvalue": "1",
                                "type": "dropdown"
                            }
                        ]
                    },
                    {
                        "type": "featuremenu",
                        "model": "flavor",
                        "label": "Flavor Selector",
                        "order": 0,
                        "children": [
                            {
                                "options": [
                                    {
                                        "label": "Right",
                                        "value": "right"
                                    },
                                    {
                                        "label": "Center",
                                        "value": "center"
                                    },
                                    {
                                        "label": "Left",
                                        "value": "left"
                                    }
                                ],
                                "initvalue": "right",
                                "type": "radio",
                                "model": "align"
                            },
                            {
                                "model": "priority",
                                "label": "Display Priority",
                                "options": [
                                    {
                                        "label": "Low",
                                        "value": "0"
                                    },
                                    {
                                        "label": "Medium",
                                        "value": "1"
                                    },
                                    {
                                        "label": "High",
                                        "value": "2"
                                    }
                                ],
                                "initvalue": "1",
                                "type": "dropdown"
                            }
                        ]
                    },
                    {
                        "type": "featuremenu",
                        "model": "playbackRate",
                        "label": "Playback Rate Selector",
                        "order": 0,
                        "children": [
                            {
                                "options": [
                                    {
                                        "label": "Right",
                                        "value": "right"
                                    },
                                    {
                                        "label": "Center",
                                        "value": "center"
                                    },
                                    {
                                        "label": "Left",
                                        "value": "left"
                                    }
                                ],
                                "initvalue": "right",
                                "type": "radio",
                                "model": "align"
                            },
                            {
                                "model": "priority",
                                "label": "Display Priority",
                                "options": [
                                    {
                                        "label": "Low",
                                        "value": "0"
                                    },
                                    {
                                        "label": "Medium",
                                        "value": "1"
                                    },
                                    {
                                        "label": "High",
                                        "value": "2"
                                    }
                                ],
                                "initvalue": "1",
                                "type": "dropdown"
                            }
                        ]
                    },
                    {
                        "type": "featuremenu",
                        "model": "flagContent",
                        "label": "Flag Content",
                        "order": 0,
                        "children": [
                            {
                                "options": [
                                    {
                                        "label": "Right",
                                        "value": "right"
                                    },
                                    {
                                        "label": "Center",
                                        "value": "center"
                                    },
                                    {
                                        "label": "Left",
                                        "value": "left"
                                    }
                                ],
                                "initvalue": "right",
                                "type": "radio",
                                "model": "align"
                            },
                            {
                                "model": "priority",
                                "label": "Display Priority",
                                "options": [
                                    {
                                        "label": "Low",
                                        "value": "0"
                                    },
                                    {
                                        "label": "Medium",
                                        "value": "1"
                                    },
                                    {
                                        "label": "High",
                                        "value": "2"
                                    }
                                ],
                                "initvalue": "1",
                                "type": "dropdown"
                            }
                        ]
                    }
                ]
            },
            {
                "type": "menu",
                "model": "watermark",
                "label": "Watermark",
                "children": [
                    {
                        "label": "Watermark Logo URL",
                        "model": "watermarkURL",
                        "validation": "url",
                        "type": "text"
                    },
                    {
                        "options": [
                            {
                                "label": "Top Left",
                                "value": "top,left"
                            },
                            {
                                "label": "Top Right",
                                "value": "top,right"
                            }
                        ],
                        "label": "Position",
                        "type": "dropdown",
                        "initvalue": "top,left",
                        "model": "watermarkPosition"
                    }
                ]
            },
            {
                "type": "menu",
                "model": "stylesAndColors",
                "label": "Styles & Colors",
                "children": [
                    {
                        "label": "ControlBar Background",
                        "model": "controlBarColor",
                        "default": "#000",
                        "type": "color"
                    },
                    {
                        "label": "Playhead Scurbber",
                        "model": "playheadColor",
                        "default": "#fff",
                        "type": "color"
                    },
                    {
                        "label": "Focus Color",
                        "model": "focusColor",
                        "default": "#fff",
                        "type": "color"
                    },
                    {
                        "type": "featuremenu",
                        "model": "textOptions",
                        "label": "Text Options",
                        "children": [
                            {
                                "label": "Text Color",
                                "model": "textColorValue",
                                "default": "#fff",
                                "type": "color"
                            },
                            {
                                "options": [
                                    {
                                        "label": "Ariel",
                                        "value": "ariel"
                                    },
                                    {
                                        "label": "Helvetica",
                                        "value": "helvetica"
                                    }
                                ],
                                "label": "Font name",
                                "type": "dropdown",
                                "initvalue": "ariel",
                                "model": "fontName"
                            },
                            {
                                "label": "Base Font Size (ems)",
                                "type": "number",
                                "default": 1,
                                "model": "fontSize",
                                "from": 0.1,
                                "to": 5,
                                "stepsize": 0.1,
                                "numberOfDecimals": 1
                            }
                        ]
                    }
                ]
            }
